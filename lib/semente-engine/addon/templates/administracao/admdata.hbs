<!-- INÍCIO DE TODOS EM UMA INSTITUIÇÃO -->
<section class="report__section report-people report__section--is-active" id="report-people">


  <div class="flex flex-col">

    <div class="filters">
      <!-- flex flex-row items-start justify-between m-bottom--sm -->


      {{#if hasChildren}}
