{{#if aulasAno}}
    <li>
        Ano: <strong>{{ano}}</strong>
    </li>

    <li>
        Segmento: <strong>{{aulasAno.firstObject.plataformaAno.segmento.titulo}}</strong>
    </li>

    <li>
        Atrelado a <strong>{{aulasAno.length}}</strong> aulas |
        {{#each aulasAno as |aulaA index|}}
            {{#if (islower index 5)}}
                <span class="title-aux">{{aulaA.titulo}}</span>
            {{/if}}
            {{#if (equalstr index 6)}}
                <span class="title-aux">...</span>
            {{/if}}
        {{/each}}
    </li>
{{/if}}
