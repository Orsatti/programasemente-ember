{{#if aulasAno}}
    <span class="title-aux">Ano: <strong>{{ano}}</strong></span>
    <span class="title-aux">Segmento: <strong>{{aulasAno.firstObject.plataformaAno.segmento.titulo}}</strong></span>
    <span class="title-aux">Atrelado a <strong>{{aulasAno.length}}</strong> aulas | </span>
    {{#each aulasAno as |aulaA index|}}
        {{#if (islower index 5)}}
            <span class="title-aux">{{aulaA.titulo}}</span>
        {{/if}}
        {{#if (equalstr index 6)}}
            <span class="title-aux">...</span>
        {{/if}}
    {{/each}}
{{/if}}
