<ul class="callout flow-lg">
    <li class="flex flex-col">
        <span class="text-aux m-bottom--xxs">E-mail</span>
        <span class="fw--4">{{instrutor.emailCadastrado}}</span>
    </li>
    <li class="flex flex-col">
        <span class="text-aux m-bottom--xxs">Nome completo</span>
        <span class="fw--4">{{instrutor.name}}</span>
    </li>
    {{#if instrutor.telefone}}
    <li class="flex flex-col">
        <span class="text-aux m-bottom--xxs">Celular</span>
        <span class="fw--4">{{instrutor.telefone}}</span>
    </li>
    {{/if}}
    <li class="flex flex-col">
     <span class="text-aux m-bottom--xxs">Tipo de professor</span>
     <span class="fw--4">{{if instrutor.isAplicador "Aplicador" "NÃ£o aplicador"}}</span>
    </li>
    <li class="flex flex-col">
    {{#if instrutor.plataformaAnos}}
            <span class="text-aux m-bottom--xxs">Anos nos quais aplica</span>
        {{#each instrutor.plataformaAnos as |pa| }}
            <span class="fw--4 m-top--xxs">{{pa.name}}</span>
        {{/each}}
    {{/if}}

    </li>
</ul>

